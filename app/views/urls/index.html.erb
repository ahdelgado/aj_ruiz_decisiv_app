<% provide(:title, 'All URLs') %>
<h1>List of URLs</h1>

<%= will_paginate %>

<table style="width:50%">
  <tr>
    <td><h2>Long URL</h2></td>
    <td><h2>Generated Short URL<h2></td> 
  </tr>
  <% @urls.each do |url| %>
  <tr>
    <td>
      <center><%= link_to url.long_url, url %></center>
    </td>
    <td>
      <center><%= link_to url.short_url, url %></center>
    </td>
  </tr>
  <% end %>
</table>

<%= will_paginate %>
